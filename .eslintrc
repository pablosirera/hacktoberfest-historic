{
  "env": {
    "es2022": true,
    "node": true,
    "browser": true
  },
  "extends": [
    "eslint:recommended",
    "eslint-config-prettier",
    "plugin:astro/recommended",
    "plugin:vue/vue3-essential"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-multiple-empty-lines": "off",
    "quotes": ["warn", "single"],
    "eol-last": "off"
  },
  "overrides": [
    {
      "files": ["*.astro"],
      "parser": "astro-eslint-parser",
      // "parserOptions": {
      //   "parser": "@typescript-eslint/parser",
      //   "extraFileExtensions": [".astro"]
      // },
      "rules": {}
    }
  ],
  "plugins": ["vue"]
}
